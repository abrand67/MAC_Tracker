# Common
DB_BACKEND=postgres  # or: mongo
SNMP_VERSION=v2c     # or: v3
THREAD_COUNT=10

# v2c Settings
SNMP_COMMUNITY=public

# v3 Settings
SNMP_V3_USER=snmpuser
SNMP_V3_AUTH_KEY=myauthpass
SNMP_V3_AUTH_PROTO=SHA  # or MD5
SNMP_V3_PRIV_KEY=myprivkey
SNMP_V3_PRIV_PROTO=AES  # or DES

# PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mac_tracking
DB_USER=postgres
DB_PASSWORD=yourpassword

# MongoDB
MONGO_URI=mongodb://localhost:27017
MONGO_DB=mac_tracking
